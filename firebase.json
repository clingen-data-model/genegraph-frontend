{
    "hosting": {
	"public": "public",
	"ignore": [
	    "firebase.json",
	    "**/.*",
	    "**/node_modules/**"
	],
	"rewrites": [
	    {
		"source": "/api",
		"destination": "https://genegraph-api.stage.clingen.app/api"
	    },
	    {
		"source": "**",
		"destination": "/index.html"
	    }
	]
    }
}
